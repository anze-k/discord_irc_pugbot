{
    "env": {
        "node": true,
        "es2020": true
    },
    "extends": ["prettier", "eslint:recommended"],
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "semi": ["error", "always"]
    }
}
